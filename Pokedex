import java.util.ArrayList;

public class Pokedex {

    private ArrayList<Pokemon> myPokedex = new ArrayList<>();

    public void addToDex(Pokemon pokemon){

        myPokedex.add(pokemon);
    }


    public String toString() {

        String str = "";
        for(Pokemon p : myPokedex)
            str += p.toString() + "\n";
        return str;
    }
}
